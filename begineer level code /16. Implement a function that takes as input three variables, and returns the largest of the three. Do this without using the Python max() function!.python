def largest_of_three(a, b, c):
    largest = a

    if b > largest:
        largest = b
    if c > largest:
        largest = c

    return largest


x = int(input("Enter first number: "))
y = int(input("Enter second number: "))
z = int(input("Enter third number: "))

print("Largest number is:", largest_of_three(x, y, z))
